{
    "name": "study extension",
    "version": "0.1",
    "description": "this extension is for studying",
    "manifest_version": 2,
    "web_accessible_resources": [
        "pictures/*.jpg"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_icon": "pictures/icon.png"
    }
}